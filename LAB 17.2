#include <iostream>
using namespace std;

struct ListNode 
{
	int data;
	ListNode *next;
};

void insert(struct ListNode **head, int data)	
{
	struct ListNode *n = (struct ListNode*) malloc(sizeof(struct ListNode));
	n->data = data;
	n->next = *head;
	*head = n;
}


void printList(struct ListNode *head)
{
	struct ListNode *temp = head;
	while (temp != NULL)
	{
		cout << temp->data << "  ";
		temp = temp->next;
	}
	cout << endl;
}

int main(int argc, char *argv[])
{

	struct ListNode *head = NULL;		// 
	
	insert(&head, 10);
	insert(&head, 8);
	insert(&head, 3);
	

	printList(head);

	cout << "The number of node is " << (&head, 3) << endl;

	system("pause");

	return 0;
}
